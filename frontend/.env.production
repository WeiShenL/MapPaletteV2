# MapPalette V2 - Production Environment Variables
# Copy this file to .env.production.local and fill in your production values

# ==================== SUPABASE ====================
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key-here

# ==================== API ENDPOINTS ====================
# Replace with your production API URLs
VITE_API_URL=https://api.mappalette.com

# Microservices (if using separate domains)
VITE_USER_SERVICE_URL=https://api.mappalette.com/users
VITE_FEED_SERVICE_URL=https://api.mappalette.com/feed
VITE_POST_SERVICE_URL=https://api.mappalette.com/posts
VITE_INTERACTION_SERVICE_URL=https://api.mappalette.com/interactions
VITE_FOLLOW_SERVICE_URL=https://api.mappalette.com/follow
VITE_PROFILE_SERVICE_URL=https://api.mappalette.com/profile
VITE_SOCIAL_INTERACTION_SERVICE_URL=https://api.mappalette.com/social
VITE_EXPLORE_ROUTES_SERVICE_URL=https://api.mappalette.com/explore
VITE_LEADERBOARD_SERVICE_URL=https://api.mappalette.com/leaderboard
VITE_USER_DISCOVERY_SERVICE_URL=https://api.mappalette.com/discovery

# ==================== GOOGLE MAPS ====================
VITE_GOOGLE_MAPS_API_KEY=your-production-google-maps-api-key

# ==================== APPLICATION ====================
VITE_APP_NAME=MapPalette
VITE_APP_URL=https://mappalette.com
VITE_APP_ENV=production

# ==================== FEATURES ====================
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_DEBUG=false
